{
    "hosting": {
  
      "public": "www", 
  
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
  
  
      "headers": [ {
        "source": "/www/**",
        "headers": [ {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        } ]
      }  ]
      
  
    }
  }